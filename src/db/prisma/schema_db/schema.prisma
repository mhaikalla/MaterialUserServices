generator client {
  provider        = "prisma-client-js"
  output          = "../generated/output"
  previewFeatures = ["interactiveTransactions"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_USER_URL")
}

model Users {
  id                    Int                @id @default(autoincrement())
  email                 String             
  first_name            String     
  last_name             String?             
  password              String
  token                 String?            
  phone                 String?            
  created_at            DateTime           @default(now())
  updated_at            DateTime?          
  deleted_at            DateTime?         
  @@map(name: "users")
}


model UserAdmins {
  id                    Int                @id @default(autoincrement())
  email                 String             
  name                  String             
  password              String           
  created_at            DateTime           @default(now())
  updated_at            DateTime?          
  deleted_at            DateTime?          
  @@map(name: "user_admins")
}